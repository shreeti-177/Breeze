<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.personalfinance">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PersonalFinance">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".OnboardActivity" />
        <activity android:name=".HomePageActivity" />
        <activity android:name=".BudgetActivity" />
        <activity android:name=".CashTransactionActivity" />
        <activity android:name=".ExpenseActivity" />
        <activity android:name=".HabitsActivity" />
        <activity android:name=".PlansActivity" />


    </application>

</manifest>